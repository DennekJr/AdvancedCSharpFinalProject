@model IEnumerable<AdvancedCFinalProject.Data.ApplicationUser>

@{
    ViewData["Title"] = "DisplayAllUsers";
}

<h1>Display All Users In Role</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserName)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
        </tr>
}
    </tbody>
</table>
